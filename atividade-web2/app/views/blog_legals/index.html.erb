<p id="notice"><%= notice %></p>

<h1>Blog Legals</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blog_legals.each do |blog_legal| %>
      <tr>
        <td><%= blog_legal.titulo %></td>
        <td><%= blog_legal.text %></td>
        <td><%= link_to 'Show', blog_legal %></td>
        <td><%= link_to 'Edit', edit_blog_legal_path(blog_legal) %></td>
        <td><%= link_to 'Destroy', blog_legal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Blog Legal', new_blog_legal_path %>
